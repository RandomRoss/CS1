#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int t = 0;
    int count1 = 2;
    int sum1 = 0; 
    for (t=0;t<= 11;t++){
        sum1 = sum1 + count1;
        count1 = count1 + 3;
    }
    
    cout << "Sum: " << sum1 << endl; 
    
    int i = 0;
    int count2 = 2;
    int sum2 = 0;
    int average = 0;
    for (count2 = 2;count2<=40;count2 += 2){
        
        sum2 = sum2 + count2;
        i = i + 1; 
        
        
    }
    average = sum2/i;
    cout << "Average: " << average << endl;
    
    double count3 = 25;
    int i2 = 0;
    cout << count3 ;
    for (i2=0;i2<50;i2++){
        count3 = count3 - .5;
        cout << "," << count3;
    }
    
    return 0;
}
